{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true,
  },
  "parserOptions": {
    "sourceType": "module",
  },
  "globals": {
    "browser": false,
  },
  "extends": [
    "eslint:recommended", "google",
  ],
  "rules": {
    "no-console": 1,
    "valid-jsdoc": 1,
  },
}
